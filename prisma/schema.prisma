
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Project {
  id         Int     @id @default(autoincrement())
  repo      String
  prs      String
  issues String
  project   String
  state     String

  @@unique([repo, prs], name: "base")
}


